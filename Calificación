#include <stdio.h>

int main() {
    float cal;
    
    printf("Programa que indique si un estudiante aprueba un Examen");
    printf("Ingrese la calificacion que obtuvo ");
     scanf("%f",& cal);
        if (cal >= 8) {
            printf("Aprobado");
        }
       else if (cal >=6) {
                printf("Reprobado");
            }
}
